{
  "services": [
    {
        "id": "grafana",
        "name": "grafana",
        "tags": ["http", "v3"],
        "port": 3000,
        "checks": [{
            "script": "supervisorctl status|egrep 'grafana3.*RUNNING' || supervisorctl status grafana",
            "interval": "10s"
        }]
    }
  ]
}
